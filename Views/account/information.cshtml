                        <div class="Block">
                            <div class="defaultTitle MyAccountTitle ">
                                <h1>
                                    Thông tin tài khoản
                                </h1>
                                <br />
                            </div>
                            <div class="defaultContent BlockContent MyAccountContent">
                                <br />
                                
                                <div class="FormContainer HorizontalFormContainer NarrowFormContainer">
                                    <dl>

                                        <dt><span class="required">*</span> Họ đệm:</dt>
                                        <dd>
                                            <input type="text" class="NormalTextBox" style="width:200px;">
                                            <span style="display:none;">Yêu cầu nhập thông tin Họ đệm</span>
                                        </dd>
                                        <br />
                                        <dt><span class="required">*</span> Tên:</dt>
                                        <dd>
                                            <input type="text" class="NormalTextBox" style="width:200px;">
                                            <span style="display:none;">Yêu cầu nhập thông tin Tên</span>
                                        </dd>
                                        <br />
                                        <dt>Ngày sinh:</dt>
                                        <dd>
                                            <input type="text" class="TextInput hasDatepicker" style="width: 100px;" value="01/01/1990"><img class="ui-datepicker-trigger" alt="..." title="...">&nbsp;
                                        </dd>
                                        <br />
                                        <dt>Công ty:</dt>
                                        <dd>
                                            <input type="text" class="NormalTextBox" style="width:350px;">
                                        </dd>
                                        <br />
                                        <dt><span class="required">*</span> Điện thoại:</dt>
                                        <dd>
                                            <input type="text" class="NormalTextBox" style="width:200px;">
                                            <span style="display:none;">Yêu cầu nhập thông tin Điện thoại liên hệ</span>
                                        </dd>
                                        <br />
                                        <dt><span class="required">*</span> Email:</dt>
                                        <dd>
                                            <input type="text" class=" NormalTextBox" style="width:350px;">
                                        </dd>
                                        <br />
                                        <dt>Yahoo:</dt>
                                        <dd>
                                            <input type="text" class="NormalTextBox" style="width:350px;">
                                        </dd>
                                        <br />
                                        <dt>Skype:</dt>
                                        <dd>
                                            <input type="text" class="NormalTextBox" style="width:350px;">
                                        </dd>
                                        <br />
                                        <dt>Gtalk:</dt>
                                        <dd>
                                            <input type="text" class="NormalTextBox" style="width:350px;">
                                        </dd>
                                        <br />
                                        <dt>Facebook:</dt>
                                        <dd>
                                            <input type="text" class="NormalTextBox" style="width:350px;">
                                        </dd>
                                        <br />
                                        <dt>&nbsp;</dt>
                                        <dd>
                                            <input type="submit" value="Cập nhật" onclick="javascript:WebForm_DoPostBackWithOptions(new WebForm_PostBackOptions(&quot;ctl00$cphMain$ctl00$ctl01$ctl00$btnUpdate&quot;, &quot;&quot;, true, &quot;UpdateBasicInfo&quot;, &quot;&quot;, false, false))" id="cphMain_ctl00_ctl01_ctl00_btnUpdate" class="CreateAccountButton">
                                            <input type="submit" value="Quay lại" id="cphMain_ctl00_ctl01_ctl00_btnBack" class="CreateAccountButton">
                                        </dd>
                                    </dl>
                                </div>
                                <br />
