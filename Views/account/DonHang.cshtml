
@{
    ViewBag.Title = "DonHang";
}


<link href="@Url.Content("~/Content/styleDangKi.css")" rel="stylesheet" type="text/css" />

<div style="width:1170px; margin:0px auto; margin-top:10px;">
    <link href="@Url.Content("~/Content/styleIndex.css")" rel="stylesheet" type="text/css" />


    <section>
        <link href="@Url.Content("~/Content/Css/animate.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/Css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/Css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/Css/main.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/Css/prettyPhoto.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/Css/price-range.css")" rel="stylesheet" type="text/css" />
        <link href="@Url.Content("~/Content/Css/responsive.css")" rel="stylesheet" type="text/css" />

        <div class="container">
            <div class="row">
                <div class="col-sm-3">
                    <div class="left-sidebar">

                        <div class="panel-group category-products" id="accordian">
                            <!--category-productsr-->
                            <div class="panel panel-default">
                                <div class="left">
                                    <div id="vertical-menu">
                                        <div class="defaultTitle cate-menu-title">
                                            <span>Tài Khoản của tôi</span>

                                            <div class="left">
                                                <a href="../account/" title="Lê Triệu">

                                                </a>
                                            </div>
                                        </div>
                                        <div class="defaultContent cate-menu-content">
                                            <ul>
                                                <li>
                                                    <a href="../account/information">
                                                        Thông tin chung tài khoản
                                                    </a>
                                                </li>

                                                <li>
                                                    <a href="../account/">
                                                        Thông báo của tôi
                                                    </a>
                                                </li>

                                                <li>
                                                    <a href="../account/DonHang">
                                                        Đơn hàng của tôi
                                                    </a>
                                                </li>

                                                <li>
                                                    <a href="../account/index">
                                                        Hỏi đáp
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div><!--/category-productsr-->








                    </div>
                </div>

                <div class="col-sm-9 padding-right">
                    <div class="features_items">
                        <!--features_items-->
                        <!--=======================================================================================-->
                        <div id="main" class="col-main col-md-9 b-content" role="main">

                            <link type="text/css" rel="stylesheet" href="./Đơn hàng của tôi _ Tiki.vn_files/thickbox.css">
                            <script type="text/javascript" src="./Đơn hàng của tôi _ Tiki.vn_files/sale.orders.js"></script>
                            <script type="text/javascript" src="./Đơn hàng của tôi _ Tiki.vn_files/thickbox.js"></script>
                            <div class="b-sales">

                                <div class="b-my-account__head">
                                    <h1 class="b-my-account__title">Đơn hàng của tôi</h1>
                                </div>
@{
                                            foreach(WebApplication2.DAO.DonHang temp in ViewBag.DSDonHang)
                                            {
                                <table class="table table-striped table-bordered" id="my-orders-table">
                                    <thead>
                                        <tr class="first last">
                                            <th>Đơn hàng #</th>
                                            <th>Ngày mua</th>
                                            <th>Gửi đến</th>
                                            <th>Tổng tiền</th>
                                            <th>Trạng thái đơn hàng</th>
                                            <th>&nbsp;</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        
                                                
                                          
                                        <tr class="first last odd">
                                            <td><a href="../" title="Xem đơn hàng"> @temp.MaHoaDon</a></td>
                                            <td>@temp.NgayGiaoDich</td>
                                            <td>@temp.NguoiGiaoDich</td>
                                            <td><span class="price">@temp.TongTien&nbsp;₫</span></td>
                                            <td>
                                                <a href="../" title="Xem đơn hàng">
                                                    <em>@temp.TinhTrang</em>
                                                </a>
                                            </td>
                                            <td class="last">
                                                <a href="../">Xem đơn hàng</a>
                                                <!--                        <a href="/minify/tiki_ajax/hso/getCouponByOrderId" class="get-coupon fancybox.ajax" data-order-id="" token="" style="color:red;">
                                    <em>(Nhận mã coupon)</em>
                                </a>-->
                                            </td>
                                        </tr>
                                            
                                      
                                    </tbody>
                                </table>

}
                                }


                            </div>


                            

                        </div><!--/ .b-content-->

                        <!--lặp lại-->
                        <!--=======================================================================================-->

                        
                    </div><!--features_items-->
                </div>
            </div>
        </div>
    </section>
</div>

<!--=============================================================================-->


